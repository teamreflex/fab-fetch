{
  "name": "fab-fetch",
  "version": "1.0.0",
  "description": "Fetch and archive content from Fab.",
  "main": "src/app.ts",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "fetch": "node --experimental-specifier-resolution=node --loader ts-node/esm src/app.ts",
    "fetch-without-posting": "node --experimental-specifier-resolution=node --loader ts-node/esm src/app.ts --no-posting",
    "dev": "node --experimental-specifier-resolution=node --loader ts-node/esm src/app.ts --dev",
    "dev-without-posting": "node --experimental-specifier-resolution=node --loader ts-node/esm src/app.ts --dev --no-posting"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teamreflex/fab-fetch.git"
  },
  "author": "Kyle Ward <kairu@team-reflex.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/teamreflex/fab-fetch/issues"
  },
  "homepage": "https://github.com/teamreflex/fab-fetch#readme",
  "dependencies": {
    "@types/luxon": "^2.0.9",
    "chalk": "^5.0.0",
    "dotenv": "^16.0.0",
    "luxon": "^2.3.1",
    "node-fetch": "^3.2.0",
    "ts-node": "^10.5.0",
    "twitter-api-v2": "^1.11.0",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4"
  }
}
